<Window x:Class="HMS_Software_V2.UserCommon_Forms.RequestAppointment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HMS_Software_V2.UserCommon_Forms"
        mc:Ignorable="d"
        MinWidth="1280" MinHeight="720"
        Title="RequestAppointment" Height="720" Width="1280" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="100"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <ScrollViewer VerticalScrollBarVisibility="Auto" Grid.Row="0" Grid.RowSpan="1" Margin="10,70,20,5">
            <WrapPanel x:Name="D_Clinictypes_WrapP" Margin="1,0,1,1" Background="Transparent"/>
        </ScrollViewer>

        <ScrollViewer VerticalScrollBarVisibility="Auto" Grid.Row="0" Grid.RowSpan="1" Grid.Column="1" Margin="10,70,20,5">
            <WrapPanel x:Name="D_RequestedClinics_WrapP" Margin="1,0,1,1" Background="Transparent"/>
        </ScrollViewer>

    </Grid>
</Window>
